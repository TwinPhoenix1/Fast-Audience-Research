root ::= ThreadData
ThreadData ::= "{"   ws   "\"goals\":"   ws   string   ","   ws   "\"frustrations\":"   ws   stringlist   ","   ws   "\"satisfaction\":"   ws   stringlist   "}"
ThreadDatalist ::= "[]" | "["   ws   ThreadData   (","   ws   ThreadData)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"